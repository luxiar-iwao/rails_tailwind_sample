<div id="<%= dom_id staff %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= staff.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Position:</strong>
    <%= staff.position %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= staff.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= staff.phone %>
  </div>
</div>
